# Uncomment the next line to define a global platform for your project
# platform :ios, '13.0'

def pods_for_debugs
  pod 'SwiftLint', :configurations => ['Debug']
end

target 'Ketchup' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  inhibit_all_warnings!

  # Pods for Ketchup
  pods_for_debugs()
  pod 'SnapKit', '~> 5.6.0'
end
